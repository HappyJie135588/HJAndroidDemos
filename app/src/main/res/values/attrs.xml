<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Topbar">
        <attr name="title" format="string" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="titleTextColor" format="color" />
        <attr name="titleBackground" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="SettingItemView">
        <attr name="destext" format="string" />
        <attr name="offtest" format="string" />
        <attr name="ontest" format="string" />
    </declare-styleable>
    <declare-styleable name="TouchPullView">
        <attr name="pColor" format="color" /><!--颜色-->
        <attr name="pRadius" format="dimension" /><!--半径-->
        <attr name="pDragHeight" format="dimension"/>拉的高度
        <attr name="pTangentAngle" format="integer" />角度
        <attr name="pTargetWidth" format="dimension" />宽度
        <attr name="pTargetGravityHeight" format="dimension" />重心高度
        <attr name="pContentDrawable" format="reference" />
        <attr name="pContentDrawableMargin" format="dimension" />
    </declare-styleable>
</resources>
